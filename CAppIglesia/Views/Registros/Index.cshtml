@{
    ViewData["Title"] = "Sistema Contable IBPD";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br />
<div class="container">
    <div class="row">
        <div class="col-6 ">
            <h4> Datos de Diezmos</h4>
        </div>
        <div class="col-6 text-right ">
            <a asp-action="Create" class="btn btn-success">
                <i class="fas fa-plus">Agregar Un Donante</i>
            </a>
        </div>

    </div>
    <br />
    <br />
    <div class="col">
        <table id="tbltablaR" class="table table-bordered table-striped  " style="width:100%">

            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Concepto</th>
                    <th>Cantidad</th>
                    <th>Fecha</th>
                    <th>Opcions</th>

                </tr>
            </thead>


        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/Registro.js">

    </script>

}

